{
  "$schema": "..\\..\\node_modules\\nx\\schemas\\project-schema.json",
  "sourceRoot": "apps/bpd-web",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/web:webpack",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "compiler": "babel",
        "outputPath": "dist/apps/bpd-web",
        "index": "apps/bpd-web/public/index.html",
        "baseHref": "/",
        "main": "apps/bpd-web/src/index.tsx",
        "polyfills": "apps/bpd-web/polyfills.ts",
        "tsConfig": "apps/bpd-web/tsconfig.app.json",
        "assets": [
          {
            "input": "apps/bpd-web/src/assets",
            "glob": "*",
            "output": "./assets"
          },
          {
            "input": "apps/bpd-web/src/assets/images",
            "glob": "*",
            "output": "./assets/images"
          },
          {
            "input": "apps/bpd-web/src/assets/carto-icons/v3",
            "glob": "*",
            "output": "./assets/carto-icons/v3"
          },
          {
            "input": "apps/bpd-web/public",
            "glob": "*",
            "output": "."
          }
        ],
        "styles": ["apps/bpd-web/src/assets/styles.less"],
        "scripts": [],
        "webpackConfig": "apps/bpd-web/webpack.config.js",
        "w": "@nrwl/react/plugins/webpack"
      },
      "configurations": {
        "development": {
          "extractLicenses": false,
          "optimization": false,
          "sourceMap": true,
          "vendorChunk": true
        },
        "production": {
          "fileReplacements": [
            {
              "replace": "apps/bpd-web/environments/environment.ts",
              "with": "apps/bpd-web/environments/environment.prod.ts"
            }
          ],
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "webpackConfig": "apps/bpd-web/webpack.config.prod.js"
        }
      }
    },
    "dev": {
      "executor": "@nrwl/web:dev-server",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "bpd-web:build",
        "hmr": true
      },
      "configurations": {
        "development": {
          "buildTarget": "bpd-web:build:development"
        },
        "production": {
          "buildTarget": "bpd-web:build:production",
          "hmr": false
        }
      }
    },
    "start": {
      "executor": "nx:run-commands",
      "options": {
        "commands": [
          {
            "command": "npx http-serve dist/apps/bpd-web"
          }
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/bpd-web/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/bpd-web"],
      "options": {
        "jestConfig": "apps/bpd-web/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
